# MongoDB connection string
MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.mongodb.net/?retryWrites=true&w=majority&appName=Cluster0

# Bcrypt salt rounds for hashing
SALT_ROUNDS=10

# Secret key for authentication or sessions
SECRET_KEY=your_secret_key_here

# Server port
PORT=3050

# Frontend domain
DOMAIN=http://localhost:5173

# Stripe configuration
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here

# Email credentials (for nodemailer or similar)
MAIL_USER=your_email_here@gmail.com
MAIL_PASS=your_email_password_here

# Cloudinary configuration
CLOUDINARY_API_SECRET=your_cloudinary_api_secret_here
CLOUDINARY_API_KEY=your_cloudinary_api_key_here
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name_here
